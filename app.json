{
  "expo": {
    "name": "Midad",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/midad_logo_adaptive_fore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/midad_logo_adaptive_fore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ibrahimadieng.frontend"
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "442662bd-a4f0-4179-8dcf-466bf95f60df"
      }
    }
  }
}
